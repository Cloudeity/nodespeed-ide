{
    "name": "brackets",
    "description": "This module integrates [Adobe Brackets](http://brackets.io/) code editor in Node.js based web applications.",
    "version": "0.3.0",
    "author": "Boyan Rabchev <boyan@rabchev.com>",
    "contributors": [ 
        { "name": "Boyan Rabchev", "email": "boyan@rabchev.com" }
    ],
    "repository": "https://github.com/rabchev/node-brackets-module.git",
